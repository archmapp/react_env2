{
  "name": "c-r-app",
  "version": "0.1.0",
  "private": true,
  "homepage": "./",

  "dependencies": {
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-scripts": "4.0.0",
    "react-icons": "^4.1.0",
    "react-router": "5.2.0",
    "react-router-dom": "^5.2.0",
    "react-slide-routes": "0.14.0"
  },
  "devDependencies": {
    "customize-cra": "^1.0.0",
    "react-app-rewired": "^2.1.9",
    "gh-pages": "^3.2.3",
    "netlify-cli": "^8.0.6",
    "sass": "^1.45.1"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "build_NODE_ENV": "set NODE_ENV=production&& webpack",
    "netlify": "netlify deploy",
    "netlify-prod": "netlify deploy --prod",
    "netlify-help": "netlify help",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "serve": "serve -s build -p 3300",
    "serve-docs": "serve -s docs -p 3300",
    "jserver": "json-server --watch db.json --port 5000"
  },
  "eslintConfig": {
    "extends": "react-app",
    "env": {
      "browser": true,
      "shortJS": true
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}